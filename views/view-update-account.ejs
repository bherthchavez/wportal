      <div class="page-wrapper">
        <div class="container-xl">
          <!-- Page title -->
          <div class="page-header d-print-none">
            <div class="row g-2 align-items-center">
              <div class="col">
                <!-- Page pre-title -->
                <div class="page-pretitle">
                  Banking
                </div>
                <h2 class="page-title">
                  View & Update Bank Account
                </h2>
              </div>
              <!-- Page title actions -->
              <div class="col-12 col-md-auto ms-auto d-print-none">
                <div class="btn-list">
                  <a href="/bank-accounts" class="btn btn-white">
                    <!-- Download SVG icon from http://tabler-icons.io/i/plus -->
                    <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-x" width="24" height="24" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round">
                      <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
                      <line x1="18" y1="6" x2="6" y2="18"></line>
                      <line x1="6" y1="6" x2="18" y2="18"></line>
                   </svg>                   
                   Cancel
                  </a>
               <form action="/updateAccount" method="post">
                  <button type="submit" name="accountID" value="<%= accountID %>" class="btn btn-primary d-none d-sm-inline-block">
                    <!-- Download SVG icon from http://tabler-icons.io/i/plus -->
                    <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-device-floppy" width="24" height="24" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round">
                      <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
                      <path d="M6 4h10l4 4v10a2 2 0 0 1 -2 2h-12a2 2 0 0 1 -2 -2v-12a2 2 0 0 1 2 -2"></path>
                      <circle cx="12" cy="14" r="2"></circle>
                      <polyline points="14 4 14 8 8 8 8 4"></polyline>
                   </svg>                    
                   Save changes
                  </button>
                 
                </div>
              </div>

            </div>
          </div>
        </div>
        
          <div class="page-body">
            <div class="container-xl">
              <div class="card">
                <div class="card-body">
                  <div class="row">
                    <div class="col-lg-3">
                      <div class="mb-3">
                        <label class="form-label">Bank Full Name</label>
                        <input type="text" class="form-control" name="bankName" value="<%= bankName %>" autocomplete="off" required>
                      </div>
                    </div>
                    <div class="col-lg-3">
                      <div class="mb-3">
                        <label class="form-label">Account Number</label>
                        <input type="text" class="form-control" name="accountNumber" value="<%= accountNumber %>" autocomplete="off" required>
                      </div>
                    </div>
                    <div class="col-lg-3">
                      <div class="mb-3">
                        <label class="form-label">Account Owner Name</label>
                        <input type="text" class="form-control" name="ownerName" value="<%= ownerName %>" autocomplete="off" required>
                      </div>
                    </div>
                    <div class="col-lg-3">
                      <div class="mb-3">
                        <label class="form-label">Bank Email</label>
                        <input type="email" class="form-control" name="bankEmail" value="<%= bankEmail %>" autocomplete="off" required>
                      </div>
                    </div>
                   
                    <div class="col-lg-3">
                      <div>
                        <label class="form-label">Account Type</label>
                        <select class="form-select" name="accountType" required>
                          <option value="Current Account" <%=accountType==='Current Account' ? 'selected' : '' %>>Current Account</option>
                          <option value="Fixed Deposit"<%=accountType==='Fixed Deposit' ? 'selected' : '' %>>Fixed Deposit</option>
                          <option value="Fusion Account"<%=accountType==='Fusion Account' ? 'selected' : '' %>>Fusion Account</option>
                        </select>
                      </div>
                    </div>
                    <div class="col-lg-3">
                      <div>
                        <label class="form-label">Deposited</label>
                        <input type="number" class="form-control" name="deposited" value="<%= deposited %>" autocomplete="off" required>
                      </div>
                    </div>
                    <div class="col-lg-3">
                      <div>
                        <label class="form-label">Withdrawal</label>
                        <input type="number" class="form-control" name="withdrawal" value="<%= withdrawal %>" autocomplete="off" required>
                      </div>
                    </div>
                    <div class="col-lg-3">
                      <div>
                        <label class="form-label">Balance Amount</label>
                        <input type="number" class="form-control" name="balanceAmount" value="<%= balanceAmount %>" autocomplete="off" required>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </form>   
        <%- include("partial/footer") -%>