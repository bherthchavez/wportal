  <div class="page-wrapper">
    <div class="container-xl">
      <!-- Page title -->
      <div class="page-header d-print-none">
        <div class="row g-2 align-items-center">
          <div class="col">

            <h2 class="page-title">
              Create Payment Voucher
            </h2>
          </div>
         

        </div>
      </div>
    </div>
    <div class="page-body">
      <div class="container-xl">
        <div class="row row-cards">
          <div class="col-12">
            <div class="card">
              <div class="card-header hd-right">
                <div class="row align-items-center">
                  <div class="col">
                    <small class="text-muted"><i class="ti ti-building-bank"> </i>Bank Name / Account Name</small>
                    <h2 class="m-0 mt-3 text-warning"></i> Industrial Credit Bank </h2>
                    <p class=" text-muted"> DR.Nasser Abdulghani</p>
                  </div>
                  
                


                  <div class="col-auto">
                    <a href="#" class="btn btn-ghost-primary "> Check Previous Payment <i class="ti ti-chevron-right"></i></a>
                  </div>
                </div>

              </div>

              <div class="card-body">
                <div class="row mt-lg-4">
                  <div class="col-lg-6">
                    <div class="row">
                      <div class="col-6">
                        <div class="mb-3">
                          <div class="form-label">Transaction Type</div>
                          <select class="form-select">
                            <option value="1">Bank Transfer</option>
                            <option value="2">Bank Payment</option>
                            <option value="3">Internal Transfer</option>
                          </select>
                        </div>
                      </div>

                      <div class="col-6">
                        <div class="mb-4">
                          <div class="form-label">Payment Mode</div>
                          <select class="form-select">
                            <option value="Cash">Cash</option>
                            <option value="Cheque">Cheque</option>
                            <option value="Bank Transfer">Bank Transfer</option>
                          </select>
                        </div>
                      </div>

                      <div class="col-6">
                        <div class="mb-3">
                          <div class="form-label">Payment Date</div>
                          <input type="date" class="form-control ">
                        </div>
                      </div>

                      <div class="col-6">
                        <div class="mb-3">
                          <div class="form-label">Upload Document</div>
                          <input type="file" class="form-control">
                        </div>
                      </div>
                    </div>
                  </div>

                  <div class="col-lg-6">
                    <div class="mb-3">
                      <label class="form-label">Description</label>
                      <textarea class="form-control" name="example-textarea-input" rows="6"></textarea>
                    </div>
                   </div>
                
                   <div class="row">
                    <div class="col-lg-12 ">
                      <div class="col-12 mt-lg-4">
                        <div class="card">
                          <div class="table-responsive">
                            <table id="tableAddAcc" class="table tableAdd table-vcenter card-table">
                              <thead>
                                <tr>
                                  <th>Account</th>
                                  <th>Memo</th>
                                  <th class="text-end">Amount</th>
                                </tr>
                              </thead>
                              <tbody>
                                <tr>
                                  <td><select class="form-select" name="accountType"><option value="" selected>--</option> <option value="Current Account"> Current Account</option> <option value="Fixed Deposit"> Fixed Deposit </option><option value="Fusion Account"> Fusion Account </option></select></td>
                                 
                                  <td class="text-muted">
                                    <input type="text" name="accountNumber" class="form-control" />
                                  </td>
                                  <td class="text-end"><input type="text" name="accountNumber" class="form-control text-end pe-2" placeholder="0.0"/></td>
                                </tr>
        
                                <tr>
                                  <td>  
                                    <button id="insertRowAcc" href="#" class="btn btn-secondary btn-pill">Add More</button>
                                </td>
                                 
                                  <td colspan="2" class="text-end">
                                   <h3><small class="me-3"> Net Amount (QAR) </small> 0.00 </h3>
                                  </td>
                                </tr>
                              </tbody>
                            </table>
                          </div>
                    
                      </div>
                    </div>
                  </div>
              </div>
            </div>
            </div>
            <div class="card-footer">
              <div class="row align-items-center">
                <div class="col"></div>
                <div class="col-auto">
                  <a href="#" class="btn">
                    Cancel
                  </a>
                  <a href="#" class="btn btn-primary ms-3">
                    Save
                  </a>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>

    <script>
      $(function () {
        $("#insertRowAcc").on("click", function (event) {
          event.preventDefault();
            var cols = "";
            cols +=  '<tr><td><select class="form-select" name="accountType"><option value="" selected>--</option> <option value="Current Account"> Current Account</option> <option value="Fixed Deposit"> Fixed Deposit </option><option value="Fusion Account"> Fusion Account </option></select></td>';
            cols +='<td ><input type="text" name="accountNumber" class="form-control" /></td>';
            cols += '<td><input type="text" name="accountNumber" class="form-control text-end pe-2" placeholder="0.0"/></td></tr>';
    
          $('#tableAddAcc tr:last').eq(-1).before(cols);
        });
      });
    </script>
    
